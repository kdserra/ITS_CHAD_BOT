{
    "compilerOptions": {
        "outDir": "lib",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strict": true,
        "strictBindCallApply": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false
    },
    "exclude": [
        "node_modules",
        "lib"
    ]
}